#include <stdio.h>
#define MAX_SIZE 100

void main()
{
    int(arr[MAX_SIZE]);
    int i, num, j, count;
    printf("Enter the size of an array:-\n");
    scanf("%d", &num);
    printf("Enter the element of array:- ");
    for(i=1; i<num; i++)
    {
        scanf("%d", &arr[i]);
    }
    for(i=0; i<num; i++)
    {
        for(j=i+1; j<num; j++)
        {
            if(arr[i] == arr[j])
            {
                count++;
                break;
            }
        }
    }

    printf("\nTotal number of duplicate elements found in array = %d", count);

}
