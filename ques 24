#include <stdio.h>
#define Max_SIZE 100

void main()
{
    int arr[Max_SIZE];
    int i, num, sum=0;
    printf("Enter the size of an array:-\n");
    scanf("%d", &num);
    printf("Enter the elements in an array:-");
    
    for(i=1; i<num; i++)
    {
        scanf("%d", &arr[i]);
    }

    for(i=0; i<num; i++)
    {
        sum+=arr[i];
    }
    printf("Sum of all elements of array = %d", sum);

    return 0;
}
